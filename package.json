{
  "name": "isomorphic-react-redux-router-app",
  "version": "1.0.0",
  "description": "A boilerplate Isomorphic React Redux Router App",
  "proxy": "http://localhost:8080",
  "scripts": {
    "build:univ:dev": "webpack --config ./webpack.conf.dev.js --progress",
    "build:univ:prod": "webpack --config ./webpack.conf.prod.js --progress",
    "build:univ:all": "npm run build:univ:dev && npm run build:univ:prod",
    "start:univ:dev": "cross-env PORT=8080 NODE_ENV=development babel-node ./app/serverside/server.js",
    "start:univ:prod": "cross-env PORT=3030 NODE_ENV=production node ./build/prod/server/bundle-server.js",
    "test:dev": "cross-env NODE_ENV=development jest",
    "test:dev:watch": "cross-env NODE_ENV=development jest --watch"
  },
  "jest": {
    "projects": [
      "<rootDir>/jest.components.config.js",
      "<rootDir>/jest.server.config.js"
    ]
  },
  "keywords": [
    "React",
    "Universal",
    "React router",
    "Redux",
    "Tailwindcss",
    "Material-ui",
    "Express",
    "Mongoose",
    "HMR",
    "Jest"
  ],
  "author": "Mathias Rando",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@reduxjs/toolkit": "^1.8.5",
    "adm-zip": "^0.5.10",
    "bcrypt": "^5.1.0",
    "core-js": "^3.23.3",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "mongoose": "^6.6.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@redux-devtools/core": "^3.13.1",
    "@redux-devtools/extension": "^3.2.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-node-externals": "^3.0.0"
  }
}
